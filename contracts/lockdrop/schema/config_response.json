{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigResponse",
  "type": "object",
  "required": [
    "astro_token_address",
    "auction_contract_address",
    "deposit_window",
    "generator_address",
    "init_timestamp",
    "lockdrop_incentives",
    "max_lock_duration",
    "min_lock_duration",
    "owner",
    "seconds_per_week",
    "weekly_multiplier",
    "withdrawal_window"
  ],
  "properties": {
    "astro_token_address": {
      "description": "ASTRO Token address",
      "type": "string"
    },
    "auction_contract_address": {
      "description": "Bootstrap Auction contract address",
      "type": "string"
    },
    "deposit_window": {
      "description": "Deposit Window Length",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "generator_address": {
      "description": "Generator (ASTRO-UST Staking) contract address",
      "type": "string"
    },
    "init_timestamp": {
      "description": "Timestamp when Contract will start accepting LP Token deposits",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "lockdrop_incentives": {
      "description": "Total ASTRO lockdrop incentives to be distributed among the users",
      "allOf": [
        {
          "$ref": "#/definitions/Uint256"
        }
      ]
    },
    "max_lock_duration": {
      "description": "Max. no. of weeks allowed for lockup",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "min_lock_duration": {
      "description": "Min. no. of weeks allowed for lockup",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "owner": {
      "description": "Account who can update config",
      "type": "string"
    },
    "seconds_per_week": {
      "description": "Number of seconds per week",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "weekly_multiplier": {
      "description": "Lockdrop Reward multiplier",
      "allOf": [
        {
          "$ref": "#/definitions/Decimal256"
        }
      ]
    },
    "withdrawal_window": {
      "description": "Withdrawal Window Length :: Post the deposit window",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "Decimal256": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal256(1_000_000_000_000_000_000) == 1.0 The greatest possible value that can be represented is 115792089237316195423570985008687907853269984665640564039457.584007913129639935 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "Uint256": {
      "type": "string"
    }
  }
}
